{
    "name": "movies",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll --verbose",
        "eject": "react-scripts eject",
        "start": "npm run build && serve -s build",
        "format": "prettier-eslint --list-different $PWD/'src/**/*.tsx'; prettier-eslint --write $PWD/'src/**/*.tsx'",
        "type-check": "tsc --noEmit -p tsconfig.json",
        "lint": "eslint ./src/** --ext .{ts,tsx} -c .eslintrc --no-error-on-unmatched-pattern",
        "lint:fix": "eslint ./src/** --ext .{ts,tsx} -c .eslintrc --no-error-on-unmatched-pattern --fix",
        "validate": "npm run type-check && npm run lint"
    },
    "dependencies": {
        "@ant-design/icons": "^4.7.0",
        "@hookform/resolvers": "2.8.8",
        "antd": "^4.23.2",
        "axios": "0.25.0",
        "clsx": "1.1.1",
        "history": "5.2.0",
        "mobx": "6.3.13",
        "mobx-react-lite": "3.2.3",
        "node": "16.13.0",
        "react": "17.0.2",
        "react-content-loader": "6.1.0",
        "react-dom": "17.0.2",
        "react-hook-form": "7.25.0",
        "react-query": "3.34.12",
        "react-router-dom": "6.2.1",
        "react-toastify": "^8.1.0",
        "sanitize.css": "13.0.0",
        "styled-components": "5.3.3",
        "uuid": "8.3.2",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@testing-library/react-hooks": "7.0.2",
        "@testing-library/user-event": "13.5.0",
        "@types/jest": "27.4.0",
        "@types/node": "17.0.10",
        "@types/react": "17.0.38",
        "@types/react-dom": "17.0.11",
        "@types/styled-components": "5.1.21",
        "@types/uuid": "8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.10.1",
        "@typescript-eslint/parser": "^5.10.1",
        "eslint": "8.7.0",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-no-use-extend-native": "^0.5.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.28.0",
        "nock": "13.2.2",
        "prettier-eslint": "13.0.0",
        "prettier-eslint-cli": "5.0.1",
        "react-scripts": "5.0.0",
        "sass": "1.49.0",
        "serve": "13.0.2",
        "stylelint": "14.3.0",
        "stylelint-config-recess-order": "3.0.0",
        "stylelint-config-standard": "24.0.0",
        "stylelint-declaration-block-no-ignored-properties": "2.5.0",
        "stylelint-order": "5.0.0",
        "stylelint-scss": "4.1.0",
        "stylelint-selector-no-empty": "1.0.8",
        "stylelint-use-nesting": "3.0.0",
        "ts-node": "10.4.0",
        "typescript": "4.5.5"
    }
}
